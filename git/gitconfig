# TODO Set up your user information 

[alias]
	unstage = reset HEAD --
	restore = reset HEAD --
	st = status
	ds = diff --staged
	rr = rebase --continue
	ra = rebase --abort
	co = checkout
	last = log -1 HEAD
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[commit]
  template = ~/.gitmessage
[core]
	commentChar = "#"
[rebase]
	autosquash = true
[push]
	default = current
